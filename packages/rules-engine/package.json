{
  "name": "rules-engine",
  "version": "1.1.1",
  "main": "./build/cjs/index.js",
  "scripts": {
    "linter:check": "eslint -c .eslintrc . --quiet",
    "flow:check": "./node_modules/.bin/flow check ./src",
    "flow:addTypes": "flow-typed install",
    "check": "yarn run linter:check && yarn run flow:check",
    "build:flow": "gen-flow-files src --out-dir build/cjs",
    "build": "yarn run check && d2-app-scripts build && yarn run build:flow"
  },
  "dependencies": {
    "cross-env": "5.0.5",
    "d2-utilizr": "^0.2.15",
    "gen-flow-files": "^0.5.0",
    "loglevel": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-flow-strip-types": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "2.25.4",
    "flow-bin": "^0.132.0"
    },
    "module": "./build/es/index.js",
    "exports": {
        "import": "./build/es/index.js",
        "require": "./build/cjs/index.js"
    }
}
