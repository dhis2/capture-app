{
  "name": "capture-app",
  "homepage": ".",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@dhis2/d2-i18n": "^1.0.3",
    "@dhis2/d2-i18n-extract": "^1.0.7",
    "@dhis2/d2-i18n-generate": "^1.0.18",
    "@dhis2/d2-ui-app": "^1.0.4",
    "@dhis2/d2-ui-core": "^1.2.0",
    "@dhis2/d2-ui-forms": "1",
    "@dhis2/d2-ui-header-bar": "1.1.0",
    "@dhis2/d2-ui-org-unit-tree": "1.0.10",
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "@redux-offline/redux-offline": "^2.3.3",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.3",
    "d2": "29.1.5",
    "d2-manifest": "^1.0.0",
    "d2-utilizr": "^0.2.15",
    "date-fns": "^1.29.0",
    "fake-indexeddb": "^2.0.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.8",
    "jest-localstorage-mock": "^2.2.0",
    "jquery": "^3.3.1",
    "loglevel": "^1.6.1",
    "moment": "^2.22.1",
    "postcss-rtl": "^1.2.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-addons-update": "^15.6.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-infinite-calendar": "^2.3.1",
    "react-popper": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-select": "^1.2.1",
    "react-virtualized": "^9.18.5",
    "react-virtualized-select": "^3.1.3",
    "redux": "^4.0.0",
    "redux-batched-actions": "^0.2.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.10",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "manualBuild": "yarn build && mkdirp buildzip/ && pushd build && bestzip ../buildzip/NewCaptureApp.zip * && popd",
    "start": "react-app-rewired start",
    "build": "yarn localize && d2-manifest package.json ./public/manifest.webapp && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject",
    "add-flow-types": "flow-typed install",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/",
    "prestart": "yarn localize && d2-manifest package.json ./public/manifest.webapp",
    "docs": "NODE_ENV=development jsdoc -c jsdoc-conf.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn extract-pot && CI=true yarn test && git add -A ."
    }
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bestzip": "^1.1.4",
    "docdash": "^0.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-dhis2": "^3.0.7",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "expose-loader": "^0.7.5",
    "flow-bin": "^0.75.0",
    "flow-typed": "^2.4.0",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-export-default-interop": "^0.3.1",
    "mkdirp": "^0.5.1",
    "react-app-rewired": "^1.5.0",
    "redux-devtools-extension": "^2.13.2"
  },
  "manifest.webapp": {
    "version": "0.1",
    "name": "Capture",
    "description": "Capture App",
    "launch_path": "index.html",
    "icons": {
      "16": "/assets/icons/track16.png",
      "48": "/assets/icons/track48.png",
      "128": "/assets/icons/track128.png"
    },
    "developer": {
      "name": "HISP",
      "url": "http://dhis2.org"
    },
    "default_locale": "en",
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
